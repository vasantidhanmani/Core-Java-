import java.util.Scanner;

public class SwapTwoNum
{ 
   public static void main(String args[])
   {
      int num1, num2;
      Scanner sc= new Scanner(System.in);
      
      System.out.println("Enter any two numbers ");
      num1= sc.nextInt();
      num2= sc.nextInt();

      System.out.println("Before swapping:" +num +num2);

      num1= num1+num2;
      num2= num1-num2;
      num1= num1-num2;

      System.out.println("Aftter swapping:" +num +num2);

      sc.close();

   }
}

/*Output:
Enter any two numbers 55 88
Before swapping:55 88
Aftter swapping:88 55 */
